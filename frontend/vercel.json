{
    "rewrites": [
        {
            "source": "/api/keep-alive",
            "destination": "/api/keep-alive"
        },
        {
            "source": "/api/:path*",
            "destination": "https://nyayaai-backend.onrender.com/:path*"
        }
    ],
    "crons": [
        {
            "path": "/api/keep-alive",
            "schedule": "*/10 * * * *"
        }
    ]
}